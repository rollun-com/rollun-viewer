{
  "name": "rollun-com/rollun-viewer",
  "description": "Skeleton for app or lib",
  "minimum-stability": "stable",
  "license": "proprietary",
  "type": "project",
  "authors": [
    {
      "name": "avz-cmf",
      "email": "email@example.com"
    },
    {
      "name": "victorynox",
      "email": "it.proffesor02@gmail.com"
    }
  ],
  "config": {
    "fxp-asset": {
      "installer-paths": {
        "npm-asset-library": "public/assets/js"
      }
    }
  },
  "require": {
    "php": "^7.0",
    "zendframework/zend-expressive": "^2.0",
    "zendframework/zend-expressive-helpers": "^4.0",
    "zendframework/zend-stdlib": "^3.1",
    "zendframework/zend-servicemanager": "^3.3",
    "zendframework/zend-expressive-zendviewrenderer": "^1.4",
    "rollun-com/rollun-installer": "^4.0",
    "rollun-com/rollun-actionrender": "^6.0",
    "zendframework/zend-http": "^2.5",
    "zendframework/zend-i18n": "^2.7",
    "npm-asset/rollun-js": "0.x",
    "http-interop/http-middleware": "0.4.1"
  },
  "require-dev": {
    "zendframework/zend-component-installer": "^1.0",
    "zendframework/zend-config-aggregator": "^1.0",
    "zendframework/zend-expressive-fastroute": "^2.0",
    "phpunit/phpunit": "^5.7.15",
    "squizlabs/php_codesniffer": "^2.8.1",
    "zfcampus/zf-development-mode": "^3.1",
    "filp/whoops": "^2.1.7"
  },
  "autoload": {
    "psr-4": {
      "rollun\\test\\": "src/Test/src",
      "rollun\\Crud\\": "src/Crud/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "rollun\\test\\skeleton\\": "test/App"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "@development-enable"
    ],
    "development-disable": "zf-development-mode disable",
    "development-enable": "zf-development-mode enable",
    "development-status": "zf-development-mode status",
    "check": [
      "@cs-check",
      "@test"
    ],
    "clear-config-cache": "php bin/clear-config-cache.php",
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "serve": "php -S 0.0.0.0:8080 -t public public/index.php",
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
    "upload-coverage": "coveralls -v",
    "lib": "rollun\\installer\\Command::command"
  }
}
